<%- include ("partials/header")
%>

<header>
    <h1>Liveability</h1>

    <form id="form">
      <input
        type="text"
        id="search"
        placeholder="Enter location"
      />
      <button>Search</button>
    </form>
  </header>

<div class="container-flex-toggleFilter">
<button onclick="toggleFilterDisplay()" id="toggleFilter">apply filters</button></div>
<div  id ="toggle-sliders" class="off">
      <div class="form-group id="slider1">
      <span>Quality of Life: <span class="indexNo" id="ex1Val"></span></span>
      <input type="range" class="custom-range" min="0" max="100" step="1" id="ex1" value="100">
        </div>
        <div class="form-group" id="slider2">
          <span>Purchasing Power: <span class="indexNo" id="ex2Val"></span></span>
          <input type="range" class="custom-range" min="0" max="100" step="1" id="ex2" value="100">
        </div>
        <div class="form-group" id="slider3">
        <span>Cost of Living: <span class="indexNo" id="ex3Val"></span></span>
        <input type="range" class="custom-range" min="0" max="100" step="1" id="ex3" value="100">
      </div>
    <div class="form-group" id="slider4">
    <span>Safety: <span class="indexNo" id="ex4Val"></span></span>
    <input type="range" class="custom-range" min="0" max="100" step="1" id="ex4" value="100">
      </div>
      <div class="form-group" id="slider5">
      <span>Health Care: <span class="indexNo" id="ex5Val"></span></span>
      <input type="range" class="custom-range" min="0" max="100" step="1" id="ex5" value="100">
    </div>
      <div class="form-group" id="slider6">
      <span>Property Price Income Ratio: <span class="indexNo" id="ex6Val"></span></span>
      <input type="range" class="custom-range" min="0" max="10" step="1" id="ex6" value="10">
    </div>
  <div class="form-group" id="slider7">
  <span>Traffic: <span class="indexNo" id="ex7Val"></span></span>
  <input type="range" class="custom-range" min="0" max="100" step="1" id="ex7" value="100">
    </div>
    <div class="form-group" id="slider8">
    <span>Pollution: <span class="indexNo" id="ex8Val"></span></span>
    <input type="range" class="custom-range" min="0" max="100" step="1" id="ex8" value="100">
  </div>
    <div class="form-group" id="slider9">
    <span>Climate: <span class="indexNo" id="ex9Val"></span></span>
    <input type="range" class="custom-range" min="0" max="100" step="1" id="ex9" value="100">
    </div>
  </div>
</div>





<div class="container-marinas">
    <div id="marina">

    </div>
</div>


    <%- include ("partials/footer")
    %>

